{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=db;Database=VitalSenseDB;User=sa;Password=YourPassword;TrustServerCertificate=True;Connection Timeout=30;"
  },
  "Jwt": {
    "Key": "your-jwt-secret-key-should-be-at-least-32-characters",
    "Issuer": "your-app-name",
    "Audience": "your-app-audience"
  },
  "Google": {
    "ClientId": "your-google-client-id",
    "ClientSecret": "your-google-client-secret",
    "RedirectUri": "your-redirect-uri"
  },
  "Swagger": {
    "Username": "swagger-username",
    "Password": "swagger-password"
  },
  "Gemini": {
    "ApiKey": "your-gemini-api-key",
    "ApiUrl": "your-gemini-api-url"
  },
  "DataProtection": {
    "KeysDirectory": "/keys"
  }
}